{
   "name": "back",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/express": "^4.17.21",
      "@types/helmet": "^0.0.48",
      "@types/joi": "^17.2.2",
      "@types/jsonwebtoken": "^9.0.7",
      "@types/memory-cache": "^0.2.6",
      "@types/node": "^16.11.10",
      "cross-env": "^7.0.3",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "bcrypt": "^5.1.1",
      "concurrently": "^9.0.1",
      "dotenv": "^16.4.5",
      "express": "^4.21.0",
      "helmet": "^7.1.0",
      "joi": "^17.13.3",
      "jsonwebtoken": "^9.0.2",
      "memory-cache": "^0.2.0",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.20"
   },
   "scripts": {
      "dev": "cross-env NODE_ENV=dev ts-node src/index.ts",
      "prod": "cross-env NODE_ENV=prod node build/index.js",
      "build": "cross-env NODE_ENV=prod npx tsc",
      "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate ./src/migrations/migration -d ./src/data-source.ts",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts"
   }
}
